{
  "version": 2,
  "builds": [
    {
      "src": "api/analisar.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/analisar",
      "methods": ["POST"],
      "dest": "/api/analisar"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
